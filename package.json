{
	"name": "windybook",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@playwright/test": "^1.32.1",
		"@storybook/addon-essentials": "7.0.0-rc.8",
		"@storybook/addon-interactions": "7.0.0-rc.8",
		"@storybook/addon-links": "7.0.0-rc.8",
		"@storybook/addon-styling": "1.0.0-next.1",
		"@storybook/blocks": "7.0.0-rc.8",
		"@storybook/svelte": "7.0.0-rc.8",
		"@storybook/sveltekit": "7.0.0-rc.8",
		"@storybook/testing-library": "0.0.14-next.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.14.0",
		"@types/jest": "^29.5.0",
		"@typescript-eslint/eslint-plugin": "^5.57.0",
		"@typescript-eslint/parser": "^5.57.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-storybook": "^0.6.11",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.21",
		"postcss-loader": "^7.1.0",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"storybook": "7.0.0-rc.8",
		"svelte": "^3.57.0",
		"svelte-check": "^3.1.4",
		"tslib": "^2.5.0",
		"typescript": "^5.0.2",
		"vite": "^4.2.1",
		"vitest": "^0.29.8"
	},
	"type": "module"
